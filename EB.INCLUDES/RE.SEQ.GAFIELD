RE.SEQ.GAFIELD:     !
! Tidy up GAFIELD workfile data
    NBR.FLDS=DCOUNT(SCR.PARAMS.REC<19>,VM)
    FOR I=1 TO NBR.FLDS
        SEQUENCE=SCR.FLD.NAME.FLD:'/':SCR.PARAMS.REC<19,I>
        READV DUMMY FROM FG$WORK.FILE,'GAFIELD*DATA*':SEQUENCE,SF.SEQ.FLD THEN
            WRITEV I ON FG$WORK.FILE,'GAFIELD*DATA*':SEQUENCE,SF.SEQ.FLD
        END
    NEXT I
    RETURN
