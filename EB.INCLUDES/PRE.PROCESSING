    FG$FLD=0;FG$ACT='';FG$VALID=TRUE;FG$AMEND=FALSE;FG$INPUT.LIST=''
    IF FG$SCR.PRE.PROCESS THEN
        CALL @FG$SCR.VALIDATE
        IF FG$RECALC#'' THEN CALL EB_RECALC(FULL.REFRESH)
        IF FG$VALID AND NOT(FG$FINISHED) ELSE FG$FINISHED=TRUE;GOTO FINISH
        IF FG$AMEND OR NOT(FG$UPDATE) THEN
            IF FG$REFRESH THEN CALL EB_REFRESH
            CALL EB_SKIP_INIT
        END
    END
    SUB.CHANGES=FALSE
