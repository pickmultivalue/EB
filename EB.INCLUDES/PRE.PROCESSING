    FG_FLD=0;FG_ACT='';FG_VALID=TRUE;FG_AMEND=FALSE;FG_INPUT.LIST=''
    IF FG_SCR.PRE.PROCESS THEN
        CALL @FG_SCR.VALIDATE
        IF FG_RECALC#'' THEN CALL EB_RECALC(FULL.REFRESH)
        IF FG_VALID AND NOT(FG_FINISHED) ELSE FG_FINISHED=TRUE;GOTO FINISH
