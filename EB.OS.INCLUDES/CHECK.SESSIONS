  EXECUTE 'WHERE' CAPTURING OUTPUT
  FOR I=1 TO AVAIL UNTIL SESSION
    PORT=SESSIONS<1,I>
    TTY=PORT 'R %3 '
    IF INDEX(OUTPUT,TTY,1) THEN
      READV NAME FROM FG$SECURITY,PORT,6 ELSE NAME=''
      NAME=TRIM(NAME)
      IF NAME='' THEN ;! USE THIS ONE
        SESSION=I
      END
    END ELSE SESSION=I
  NEXT I
