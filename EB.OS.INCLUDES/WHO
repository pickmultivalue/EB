  FG_UDTNO=SYSTEM(18)
  FG_TLINE=SYSTEM(18)
! DUMMY=GETENV("JBCPORTNO",FG_TLINE)
  DUMMY=GETENV("PWD",FG_TACC.NAME)
  CONVERT '/\' TO CHAR(254):CHAR(254) IN FG_TACC.NAME
DC = DCOUNT(FG_TACC.NAME,CHAR(254))
  FG_TACC.NAME=FG_TACC.NAME<DC>
  FG_PRIVILEGE=3
  rc = GETENV('USER',FG_LOGNAME)
  IF NOT(rc) THEN
    rc = GETENV('USERNAME',FG_LOGNAME)
    IF NOT(rc) THEN
      FG_LOGNAME=@LOGNAME
    END
  END
