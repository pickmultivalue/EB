     OSINC$EOF=FALSE
     EXECUTE 'SELECT ACC WITH REAL-USER AND WITH NAME. LINE' CAPTURING OSINC$IO
     OSINC$WHOSON=''; OSINC$NDX=0
     LOOP
        READNEXT OSINC$PORTNO ELSE OSINC$EOF=TRUE
     UNTIL OSINC$EOF DO
        OSINC$PORTNO=TRIM(OSINC$PORTNO)
        IF OSINC$PORTNO[1,1]='*' THEN OSINC$PORTNO=OSINC$PORTNO[2,LEN(OSINC$PORTNO)]
        IF OSINC$PORTNO MATCH '1N0N' THEN
           OSINC$NDX=OSINC$NDX+1
           OSINC$WHOSON<OSINC$NDX>=OSINC$PORTNO+0
        END
     REPEAT
